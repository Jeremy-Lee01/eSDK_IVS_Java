	<servlet-name>getDeviceList</servlet-name>
	<url-pattern>/getDeviceList</url-pattern>
  </servlet-mapping>
  
  <servlet>
	<servlet-name>platformRecord</servlet-name>
	<servlet-class>com.huawei.esdk.ivs.test.servlet.PlatformRecord</servlet-class>
  </servlet>
